---
import Content from "./Content.astro"
import Middle from "./Middle.astro"
import Empty from "./Empty.astro"
const { inversed, info } = Astro.props
---

{
    inversed ? (
        <Content info={info} />
        <Middle />
        <Empty />
    ) : (
        <Empty />
        <Middle />
        <Content info={info} />
    )
}